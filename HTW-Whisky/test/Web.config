<?xml version="1.0"?>
<configuration>
    <system.web>
      <authorization>
        <deny users="?"/>
      </authorization>
    </system.web>
  <location path="OnlyYassir.aspx">
    <system.web>
      <authorization>
        <allow users="yassir"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="AllUsers.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>
  <location path="Everybody.aspx">
    <system.web>
      <authorization>
        <allow users="?"/>
      </authorization>
    </system.web>
  </location>
</configuration>
